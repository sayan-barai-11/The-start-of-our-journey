#include <stdio.h>
int main() {
    int arr[] = {2,6,54,43,76};
    int n = sizeof(arr) / sizeof(arr[0]);
    int i;  
    int largest =arr[0]; 
    for( i=1; i<n; i++){
        if (arr[i] > largest) {
             largest = arr[i];
        }   
    }
    printf("largest is :%d\n ",largest);
    return 0;
}
